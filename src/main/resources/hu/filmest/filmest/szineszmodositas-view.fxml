<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="hu.filmest.filmest.controllers.SzineszModositas"
      prefHeight="400.0" prefWidth="600.0" spacing="10">
    <Label text="Színész neve:"/>
    <TextField fx:id="modositSzinesz" promptText="Színész">
    <VBox.margin>
        <Insets bottom="20.0" />
    </VBox.margin>
    </TextField>
    <HBox alignment="CENTER">
        <Button contentDisplay="CENTER" minWidth="100.0" mnemonicParsing="false" onAction="#onModositButtonClick" prefWidth="90.0" text="Módosít" />
    </HBox>
</VBox>
