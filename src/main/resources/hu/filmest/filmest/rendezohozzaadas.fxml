      prefHeight="400.0" prefWidth="600.0" spacing="10">
    <Label text="Rendező név:"/>
    <TextField fx:id="felvetelRendezo" promptText="Rendező"/>

    <Button minWidth="100.0" mnemonicParsing="false" onAction="#onFelvetelButtonClick" prefWidth="200.0"
            text="Felvétel" />

</VBox>